{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.hrm.i.HRM_INCIDENT_001",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.hrm.i.HRM_INCIDENT_001.view.List1",
			"type": "XML",
			"async": true,
			"id": "List1"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.hrm.i.HRM_INCIDENT_001.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.hrm.i.HRM_INCIDENT_001.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"clearControlAggregation": false
			},
			"routes": [
				{
					"pattern": "",
					"name": "Main_page",
					"target": [
						"Main_page"
					]
				},
				{
					"pattern": "incidents/{objectId}",
					"name": "object",
					"target": [
						"Detail_page"
					]
				}
			],
			"targets": {
				"Main_page": {
					"viewType": "XML",
					"viewName": "Main_page",
					"viewLevel": 1
				},
				"Detail_page": {
					"viewType": "XML",
					"viewName": "Detail_page",
					"viewLevel": 2
				}
			}
		}
	}
}